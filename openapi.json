{
  "openapi": "3.0.0",
  "info": {
    "version": "0.0.1",
    "title": "Effective dates",
    "description": "# Introduction\nThis API takes in multiple params and returns an effective date based on param input."
  },
  "tags": [
    {
      "name": "Med Sup",
      "description": "Product for over 65"
    },
    {
      "name": "Med Adv",
      "description": "Product for over 65"
    },
    {
      "name": "pdp",
      "description": "Product for over 65"
    }
  ],
  "x-tagGroups": [
    {
      "name": "Market Segment",
      "tags": [
        "Under 65",
        "Over 65"
      ]
    }
  ],
  "x-servers": [
    {
      "url": "http://localhost:4200/v1",
      "description": "Localhost server dev"
    }
  ],
  "paths": {
    "/effectivedate": {
      "get": {
        "tags": [
          "Under 65",
          "Over 65"
        ],
        "summary": "Effective dates pretaining to the Under 65 market segment and products",
        "parameters": [
          {
            "name": "eventDate",
            "in": "query",
            "required": true,
            "description": "Date of event",
            "schema": {
              "type": "string",
              "example": "2019-01-01"
            }
          },
          {
            "name": "referenceDate",
            "in": "query",
            "description": "date when application was either started or product was paid for",
            "schema": {
              "type": "string",
              "example": "2019-01-01"
            }
          },
          {
            "name": "marketSegment",
            "in": "query",
            "description": "The segment of the market that you're interested in getting the effective date in.",
            "schema": {
              "type": "string",
              "example": "under65"
            }
          },
          {
            "name": "situation",
            "in": "query",
            "description": "situation for a certain rule, ex. loss of coverage",
            "schema": {
              "type": "string",
              "example": "loc"
            }
          },
          {
            "name": "product",
            "in": "query",
            "description": "The name of the product",
            "schema": {
              "type": "string",
              "example": "medsup"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EDO"
                }
              }
            }
          },
          "400": {
            "description": "The specified blank blank blank..."
          },
          "404": {
            "description": "error 404 page not found."
          },
          "default": {
            "description": "Unexpected error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "EDO": {
        "properties": {
          "Effective_dates_and_expiration_dates": {
            "type": "object",
            "example": [
              "2019-01-01",
              "2019-01-15"
            ]
          },
          "Effective_date": {
            "type": "string",
            "example": "2019-01-01"
          },
          "Market_segment": {
            "type": "string",
            "example": "Over 65"
          },
          "Situation": {
            "type": "string",
            "example": "loc"
          },
          "Product": {
            "type": "string",
            "example": "Med Sup"
          }
        }
      }
    }
  }
}
