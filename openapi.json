  "paths": {
    "/u65/ged": {
      "get": {
        "tags": [
          "General Effective Date"
        ],
        "summary": "Returns an effective date based on the given event date.",
        "parameters": [
          {
            "name": "eventDate",
            "in": "query",
            "required": true,
            "description": "Date the event happened",
            "schema": {
              "type": "string",
              "example": "2019-01-01"
            }
          },
          {
            "name": "referenceDate",
            "in": "query",
            "description": "Date when product is purchased OR if left blank then reference date would be current date",
            "schema": {
              "type": "string",
              "example": "2019-02-02"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EDO"
                }
              }
            }
          },
          "400": {
            "description": "The specified blank blank blank..."
          },
          "404": {
            "description": "error 404 page not found."
          },
          "default": {
            "description": "Unexpected error"
          }
        }
      }
    },
